<?xml version="1.0" encoding="utf-8"?>
<Policy xmlns="urn:schemas-microsoft-com:sipolicy" PolicyType="Supplemental Policy">
  <VersionEx>1.0.0.0</VersionEx>
  <BasePolicyID>{12345678-1234-1234-1234-123456789012}</BasePolicyID>
  <Rules>
    <Rule>
      <Option>Enabled:Unsigned System Integrity Policy</Option>
    </Rule>
    <Rule>
      <Option>Enabled:Audit Mode</Option>
    </Rule>
    <Rule>
      <Option>Enabled:Inherit Default Policy</Option>
    </Rule>
  </Rules>
  <!--File Rules-->
  <FileRules>
    <!--Allow Visual Studio-->
    <Allow ID="ID_ALLOW_VISUAL_STUDIO" FriendlyName="Allow Visual Studio" FileName="*" FilePath="%PROGRAMFILES%\Microsoft Visual Studio\*\Common7\IDE\*" />
    <!--Allow Visual Studio Code-->
    <Allow ID="ID_ALLOW_VS_CODE" FriendlyName="Allow Visual Studio Code" FileName="*" FilePath="%PROGRAMFILES%\Microsoft VS Code\*" />
    <!--Allow Node.js-->
    <Allow ID="ID_ALLOW_NODEJS" FriendlyName="Allow Node.js" FileName="*" FilePath="%PROGRAMFILES%\nodejs\*" />
    <!--Allow Python-->
    <Allow ID="ID_ALLOW_PYTHON" FriendlyName="Allow Python" FileName="*" FilePath="%PROGRAMFILES%\Python*\*" />
    <!--Allow Git-->
    <Allow ID="ID_ALLOW_GIT" FriendlyName="Allow Git" FileName="*" FilePath="%PROGRAMFILES%\Git\*" />
    <!--Allow Docker-->
    <Allow ID="ID_ALLOW_DOCKER" FriendlyName="Allow Docker" FileName="*" FilePath="%PROGRAMFILES%\Docker\Docker\*" />
  </FileRules>
  <!--Driver Signing Scenarios-->
  <SigningScenarios>
    <SigningScenario Value="131" ID="ID_SIGNINGSCENARIO_DRIVERS_SUPPLEMENTAL" FriendlyName="Supplemental Developer Tools Driver Scenario">
      <ProductSigners>
        <FileRulesRef>
          <FileRuleRef RuleID="ID_ALLOW_VISUAL_STUDIO" />
          <FileRuleRef RuleID="ID_ALLOW_VS_CODE" />
          <FileRuleRef RuleID="ID_ALLOW_NODEJS" />
          <FileRuleRef RuleID="ID_ALLOW_PYTHON" />
          <FileRuleRef RuleID="ID_ALLOW_GIT" />
          <FileRuleRef RuleID="ID_ALLOW_DOCKER" />
        </FileRulesRef>
      </ProductSigners>
    </SigningScenario>
    <SigningScenario Value="12" ID="ID_SIGNINGSCENARIO_WINDOWS_SUPPLEMENTAL" FriendlyName="Supplemental Developer Tools Windows Scenario">
      <ProductSigners>
        <FileRulesRef>
          <FileRuleRef RuleID="ID_ALLOW_VISUAL_STUDIO" />
          <FileRuleRef RuleID="ID_ALLOW_VS_CODE" />
          <FileRuleRef RuleID="ID_ALLOW_NODEJS" />
          <FileRuleRef RuleID="ID_ALLOW_PYTHON" />
          <FileRuleRef RuleID="ID_ALLOW_GIT" />
          <FileRuleRef RuleID="ID_ALLOW_DOCKER" />
        </FileRulesRef>
      </ProductSigners>
    </SigningScenario>
  </SigningScenarios>
  <UpdatePolicySigners>
  </UpdatePolicySigners>
  <HvciOptions>0</HvciOptions>
</Policy>